{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "collectCoverage": true,
  "coveragePathIgnorePatterns": [
    "<rootDir>/src/main/ts/interface.js"
  ],
  "collectCoverageFrom": [
    "<rootDir>/src/main/ts/**/*"
  ],
  "testMatch": [
    "<rootDir>/src/test/ts/**/*"
  ],
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json",
    "node"
  ]
}
